<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pjatk.mas.s24512.masproject.LoginController">
   <children>
      <Label text="LOGIN" />
      <TextField fx:id="loginField" />
      <Label prefHeight="0.0" prefWidth="64.0" text="PASSWORD" />
      <PasswordField fx:id="passField" />
      <Button fx:id="loginButton" mnemonicParsing="false" onAction="#login" text="LogIn" />
      <Label fx:id="infoLabel" text="Enter cridentials" textFill="RED">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="100.0" right="100.0" top="10.0" />
   </padding>
</VBox>
